name = scubox
description = scubox.com master theme
base theme = base00
screenshot = screenshot.png
engine = phptemplate
core = 7.x


;-------------------------------------------------------------------------------------
;; REGIONS
; ------------------------------------------------------------------------------------

;; Default Regions
regions[header]                                                 = Header
regions[navigation]                                             = Navigation
regions[highlighted]                                            = Highlighted
regions[help]                                                   = Help
regions[content]                                                = Content
regions[sidebar_first]                                          = First Sidebar
regions[sidebar_second]                                         = Second Sidebar
regions[footer]                                                 = Footer

;; Custom Regions
regions[hero]                                                   = Hero
regions[nav_bottom]                                             = Nav Bottom

regions[map_first]      																				= Map First
regions[map_second]     																				= Map Second
regions[map]            																				= Map
regions[content_list]            																= Content List
regions[title_bar]      																				= Title Bar

;-------------------------------------------------------------------------------------
;; STYLESHEETS // See SASS directory
; ------------------------------------------------------------------------------------

stylesheets[all][]                                              = assets/css/bootstrap.css
stylesheets[all][]                                              = assets/css/style.css

scripts[]                                                       = assets/js/scubox.js
scripts[] 																											= assets/js/jquery.easing.1.3.js
scripts[] 																											= assets/js/jquery.scrollTo-min.js
;scripts[] 																											= assets/js/jquery.overlaps.js
scripts[] 																											= assets/js/jquery.overlaps2.js
scripts[] 																											= assets/js/jquery.collision.js


